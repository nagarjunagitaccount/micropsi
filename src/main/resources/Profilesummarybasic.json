            "text": {
              "type": "string",
              "description": "Descriptive text for this profile's realestate.total_realestate_value.value bracket",
              "enum": [
                "$1-$250K",
                "$250K-$500K",
                "$500K-$750K",
                "$750K-$1MM",
                "$1MM-$2MM",
                "$2MM-$5MM",
                "$5MM-$10MM",
                "$10MM+",
                "Unable to Rate"
              ]
            },
            "min": {
              "type": "integer",
              "description": "Lowest possible real estate bracket value (always 1)",
              "enum": [
                1
              ]
            },
            "max": {
              "type": "integer",
              "description": "Highest possible real estate bracket value (always 8)",
              "enum": [
                8
              ]
            },
            "value": {
              "type": "integer",
              "description": "Value of this profile's real estate ownership bracket",
              "enum": [
                1,
                2,
                3,
                4,
                5,
                6,
                7,
                8,
                999
              ]
            }
          }
        }
      }
    },
    "locations": {
      "type": "array",
      "description": "Primary address for profile",
      "items": {
        "type": "object",
        "properties": {
          "address": {
            "type": "object",
            "description": "A mailing address for a location",
            "properties": {
              "street_line1": {
                "type": "string",
                "description": "Street address."
              },
              "street_line2": {
                "type": "string",
                "description": "Suite/Apartment."
              },
              "city": {
                "type": "string",
                "description": "City"
              },
              "state": {
                "type": "object",
                "properties": {
                  "text": {
                    "type": "string",
                    "description": "full state name (example: Texas)"
                  },
                  "value": {
                    "type": "string",
                    "description": "2 letter state code (example: TX)"
                  }
                }
              },
              "postal_code": {
                "type": "string",
                "description": "5 digit zip code"
              }
            }
          }
        }
      }
    },
