{
  "type": "object",
  "properties": {
    "profile": {
      "type": "object",
      "description": "object that contains matching profile information",
      "required": [
        "id",
        "scores"
      ],
      "properties": {
        "id": {
          "type": "integer",
          "description": "unique id of the profile"
        },
        "input": {
          "type": "object",
          "properties": {
            "first_name": {
              "type": "string",
              "description": "first name (given name)"
            },
            "last_name": {
              "type": "string",
              "description": "last name (surname)"
            },
            "address_line1": {
              "type": "string",
              "description": "house number and street"
            },
            "address_line2": {
              "type": "string",
              "description": "suite or apartment"
            },
            "city": {
              "type": "string",
              "description": "municipal center (example: Austin)"
            },
            "state": {
              "type": "string",
              "description": "abbreviated state name (example: TX)"
            },
            "zip": {
              "type": "string",
              "description": "5 digit zip code"
            },
            "phone": {
              "type": "string",
              "description": "phone number that was used in the request"
            },
            "email": {
              "type": "string",
              "description": "email address that was used in the request"
            }
          }
        },
        "scores": {
          "type": "array",
          "description": "Array of score objects. Currently always one element.",
          "items": {
            "type": "object",
            "required": [
              "score",
              "model"
            ],
            "properties": {
              "score": {
                "type": "number",
                "description": "The numeric score that matches this profile with the requested model."
              },
              "model": {
                "type": "string",
                "description": "The API model name that was provided during the request."
              },
              "ModelMappings": {
                "type": "array",
                "description": "A mapping of the input variables to the model with the source of the value from the profile data structure.\n",
                "items": {
                  "type": "object",
                  "required": [
                    "modelAttr",
                    "modelAttrDefault"
                  ],
                  "properties": {
                    "modelAttr": {
                      "type": "string",
                      "description": "Name of model input variable"
                    },
                    "modelAttrPath": {
                      "type": "string",
                      "description": "JSON path of data structure mapped into this varaible. If no path is specified, it is assumed to be provided by the client."
                    },
                    "modelAttrDefault": {
                      "type": "string",
                      "description": "Default value for input, if none provided by profile data or client input"
                    }
                  }
                }
              },
              "ModelInput": {
                "description": "The input variable names and values used for this model score",
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": true
                }
              }
            }
          }
        }
      }
    }
  },
  "$schema": "http://json-schema.org/draft-04/schema#"
}
